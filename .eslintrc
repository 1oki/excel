{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style":"off",
        "semi": "off",
        "quotes":"off",
        "indent":"off",
        "comma-dangle":"off",
        "func-call-spacing":"off",
        "comma-spacing":"off",
        "no-trailing-spaces":"off",
        "object-curly-spacing":"off",
        "arrow-parens":"off"
    },
    "extends": ["eslint:recommended", "google"]
}